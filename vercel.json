{
  "version": 2,
  "name": "qudemo",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://qudemo.com/$1",
      "permanent": false,
      "has": [
        {
          "type": "host",
          "value": "qu-demo.vercel.app"
        }
      ]
    }
  ]
}
