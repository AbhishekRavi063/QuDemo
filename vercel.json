{
  "version": 2,
  "name": "qudemo",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://qudemo.com/$1",
      "permanent": false,
      "has": [
        {
          "type": "host",
          "value": "qu-demo.vercel.app"
        }
      ]
    }
  ]
}
